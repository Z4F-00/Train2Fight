    "• Real-Time Respect Tag: Menampilkan jumlah Respect pemain lain secara instan tepat di atas karakter mereka.\n" ..
    "• Visual Indicator: Memudahkan identitas reputasi pemain lain secara langsung tanpa harus membuka menu.")
createSection("ANTI AFK & SPEED", "Sistem anti-disconnect dan kustomisasi kecepatan gerak karakter yang stabil.")

task.spawn(function()
    TweenService:Create(mainFrame, TweenInfo.new(0.5), {BackgroundTransparency = 0}):Play()
    TweenService:Create(mainStroke, TweenInfo.new(0.5), {Transparency = 0}):Play()
    TweenService:Create(headerBg, TweenInfo.new(0.5), {BackgroundTransparency = 0}):Play()
    
    task.wait(0.2)
    local introWidth = hubTitle.TextBounds.X
    logSeparator.Position = UDim2.new(0, 15 + introWidth + 5, 0, 0)
    logText.Position = UDim2.new(0, 15 + introWidth + 20, 0, 0)

    TweenService:Create(hubTitle, TweenInfo.new(0.5), {TextTransparency = 0}):Play()
    TweenService:Create(logSeparator, TweenInfo.new(0.5), {TextTransparency = 0}):Play()
    TweenService:Create(logText, TweenInfo.new(0.5), {TextTransparency = 0}):Play()

    TweenService:Create(mainFrame, TweenInfo.new(0.7, Enum.EasingStyle.Quart), {
        Size = UDim2.new(0, 450, 0, 330),
        Position = UDim2.new(0.5, -225, 0.5, -165)
    }):Play()
    
    task.wait(0.5)
    TweenService:Create(closeButton, TweenInfo.new(0.4), {TextTransparency = 0}):Play()
    TweenService:Create(creditText, TweenInfo.new(0.4), {TextTransparency = 0.5}):Play()
    line.BackgroundTransparency = 0.5
    contentContainer.Visible = true

    for _, item in ipairs(sections) do
        TweenService:Create(item.frame, TweenInfo.new(0.3), {BackgroundTransparency = 0}):Play()
        TweenService:Create(item.stroke, TweenInfo.new(0.3), {Transparency = 0.8}):Play()
        TweenService:Create(item.title, TweenInfo.new(0.3), {TextTransparency = 0}):Play()
        TweenService:Create(item.desc, TweenInfo.new(0.3), {TextTransparency = 0}):Play()
    end
end)

closeButton.MouseButton1Click:Connect(function()
    contentContainer.Visible = false
    line.BackgroundTransparency = 1
    
    TweenService:Create(closeButton, TweenInfo.new(0.2), {TextTransparency = 1}):Play()
    TweenService:Create(creditText, TweenInfo.new(0.2), {TextTransparency = 1}):Play()
    TweenService:Create(hubTitle, TweenInfo.new(0.2), {TextTransparency = 1}):Play()
    TweenService:Create(logSeparator, TweenInfo.new(0.2), {TextTransparency = 1}):Play()
    TweenService:Create(logText, TweenInfo.new(0.2), {TextTransparency = 1}):Play()
    
    local closeTween = TweenService:Create(mainFrame, TweenInfo.new(0.6, Enum.EasingStyle.Quart, Enum.EasingDirection.In), {
        Size = UDim2.new(0, 450, 0, 45),
        Position = UDim2.new(0.5, -225, 0.5, -22)
    })
    closeTween:Play()
    
    closeTween.Completed:Connect(function()
        local blur = Instance.new("BlurEffect", Lighting)
        blur.Size = 0
        
        TweenService:Create(mainFrame, TweenInfo.new(0.8), {BackgroundTransparency = 1}):Play()
        TweenService:Create(mainStroke, TweenInfo.new(0.8), {Transparency = 1}):Play()
        TweenService:Create(headerBg, TweenInfo.new(0.8), {BackgroundTransparency = 1}):Play()
        
        TweenService:Create(blur, TweenInfo.new(0.8), {Size = 15}):Play()
        local fadeInOutro = TweenService:Create(outroText, TweenInfo.new(0.8), {TextTransparency = 0})
        fadeInOutro:Play()
        
        fadeInOutro.Completed:Connect(function()
            task.wait(1)
            
            local finalFade = TweenService:Create(outroText, TweenInfo.new(0.8), {TextTransparency = 1})
            finalFade:Play()
            TweenService:Create(blur, TweenInfo.new(0.8), {Size = 0}):Play()
            
            finalFade.Completed:Connect(function()
                blur:Destroy()
                screenGui:Destroy()
            end)
        end)
    end)
end)

local dragging, dragStart, startPos
mainFrame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = mainFrame.Position
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = input.Position - dragStart
        mainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

mainFrame.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then dragging = false end
end)
