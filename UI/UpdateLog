local contentContainer = Instance.new("ScrollingFrame")
contentContainer.Size = UDim2.new(1, -20, 1, -65) 
contentContainer.Position = UDim2.new(0, 10, 0, 50) 
contentContainer.BackgroundTransparency = 1
contentContainer.ScrollBarThickness = 2
contentContainer.ScrollBarImageColor3 = Color3.fromRGB(255, 215, 0)
contentContainer.BorderSizePixel = 0
contentContainer.ClipsDescendants = true 
contentContainer.ZIndex = 1
contentContainer.Visible = false
contentContainer.Parent = mainFrame

local containerPadding = Instance.new("UIPadding", contentContainer)
containerPadding.PaddingLeft = UDim.new(0, 5) 
containerPadding.PaddingTop = UDim.new(0, 5)

local uiList = Instance.new("UIListLayout", contentContainer)
uiList.Padding = UDim.new(0, 12)
uiList.SortOrder = Enum.SortOrder.LayoutOrder

uiList:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
    contentContainer.CanvasSize = UDim2.new(0, 0, 0, uiList.AbsoluteContentSize.Y + 20)
end)

function createSection(title, desc)
    local sectionFrame = Instance.new("Frame")
    sectionFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    sectionFrame.Size = UDim2.new(0.94, 0, 0, 0) 
    sectionFrame.AutomaticSize = Enum.AutomaticSize.Y
    sectionFrame.BackgroundTransparency = 1
    sectionFrame.Parent = contentContainer
    
    local sStroke = Instance.new("UIStroke", sectionFrame)
    sStroke.Color = Color3.fromRGB(255, 215, 0)
    sStroke.Thickness = 1.2
    sStroke.Transparency = 1
    sStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border 
    
    local sTitle = Instance.new("TextLabel", sectionFrame)
    sTitle.Size = UDim2.new(1, -20, 0, 30)
    sTitle.Position = UDim2.new(0, 12, 0, 8) 
    sTitle.Text = "» " .. title
    sTitle.TextColor3 = Color3.fromRGB(255, 215, 0)
    sTitle.Font = Enum.Font.GothamBold
    sTitle.TextSize = 14
    sTitle.BackgroundTransparency = 1
    sTitle.TextTransparency = 1
    sTitle.TextXAlignment = Enum.TextXAlignment.Left

    local sDesc = Instance.new("TextLabel", sectionFrame)
    sDesc.Size = UDim2.new(1, -25, 0, 0)
    sDesc.Position = UDim2.new(0, 12, 0, 38)
    sDesc.Text = desc
    sDesc.TextColor3 = Color3.fromRGB(220, 220, 220)
    sDesc.Font = Enum.Font.Gotham
    sDesc.TextSize = 12
    sDesc.BackgroundTransparency = 1
    sDesc.TextWrapped = true
    sDesc.TextXAlignment = Enum.TextXAlignment.Left
    sDesc.TextYAlignment = Enum.TextYAlignment.Top
    sDesc.AutomaticSize = Enum.AutomaticSize.Y
    sDesc.TextTransparency = 1
    
    Instance.new("UICorner", sectionFrame).CornerRadius = UDim.new(0, 8)
    
    local uip = Instance.new("UIPadding", sectionFrame)
    uip.PaddingBottom = UDim.new(0, 15)
    
    table.insert(sections, {frame = sectionFrame, stroke = sStroke, title = sTitle, desc = sDesc})
end

createSection("Update v6.5: HONOR & RESPECT", 
    "• Show Respect System: Fitur interaksi baru untuk memberikan apresiasi kepada pemain lain.\n" ..
