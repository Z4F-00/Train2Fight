local hubName = "VOID-00"
local hubVersion = "v6.5"

local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local Lighting = game:GetService("Lighting")
local PlayerGui = Players.LocalPlayer:WaitForChild("PlayerGui")

local Check = PlayerGui:FindFirstChild(hubName.."_UpdateLog")
if Check then return end

local sections = {} 

local screenGui = Instance.new("ScreenGui")
screenGui.Name = hubName.."_UpdateLog"
screenGui.ResetOnSpawn = false
screenGui.DisplayOrder = 999999 
screenGui.Parent = PlayerGui

local mainFrame = Instance.new("Frame")
mainFrame.Name = "MainFrame"
mainFrame.Size = UDim2.new(0, 450, 0, 45)
mainFrame.Position = UDim2.new(0.5, -225, 0.5, -22)
mainFrame.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
mainFrame.BorderSizePixel = 0
mainFrame.ClipsDescendants = true 
mainFrame.BackgroundTransparency = 1
mainFrame.Parent = screenGui

local mainCorner = Instance.new("UICorner", mainFrame)
mainCorner.CornerRadius = UDim.new(0, 12)

local mainStroke = Instance.new("UIStroke", mainFrame)
mainStroke.Color = Color3.fromRGB(255, 215, 0)
mainStroke.Thickness = 2.5
mainStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
mainStroke.Transparency = 1

local headerBg = Instance.new("Frame")
headerBg.Size = UDim2.new(1, 0, 0, 45)
headerBg.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
headerBg.BackgroundTransparency = 1
headerBg.ZIndex = 5
headerBg.Parent = mainFrame
Instance.new("UICorner", headerBg).CornerRadius = UDim.new(0, 12)

