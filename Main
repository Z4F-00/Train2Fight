local mainBranch = "https://raw.githubusercontent.com/Z4F-00/Train2Fight/refs/heads/main/"
local settingsFolder = mainBranch .. "Settings/"

local MultiLink = {
    mainBranch .. "SaveLoad",
    mainBranch .. "OpeningAnim",
    settingsFolder .. "PlayerSpeed",
    settingsFolder .. "AntiAfk",
    settingsFolder .. "PVP(SafeAreaGui)",
    settingsFolder .. "ShowPlayerPower",
    settingsFolder .. "FPSTrain",
    settingsFolder .. "FPSTrainPlus",
    settingsFolder .. "Frenzy",
    settingsFolder .. "OPClick",
    settingsFolder .. "PVP",
    settingsFolder .. "Opening",
    settingsFolder .. "OpenUpdateLog"
}

for i, MultiLoad in ipairs(MultiLink) do
    local success, errorMessage = pcall(function()
        local content = game:HttpGet(MultiLoad)
        loadstring(content)()
    end)
    
    if success then
        print("Successfully loaded: " .. MultiLoad)
    else
        warn("Failed to load: " .. MultiLoad .. " | Error: " .. errorMessage)
    end
end
